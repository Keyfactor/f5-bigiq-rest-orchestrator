{
    "$schema": "https://keyfactor.github.io/v2/integration-manifest-schema.json",
    "integration_type": "orchestrator",
    "name": "F5 BigIQ",
    "status": "production",
    "support_level": "kf-supported",
    "description": "The F5 Big IQ Universal Orchestrator extension enables remote management of SSL certificates on F5 Big IQ devices, including inventory, addition, and removal of certificates, as well as deployment to linked F5 Big IP devices.",
    "link_github": true,
    "update_catalog": true,
    "release_dir": "F5BigIQ/bin/Release",
    "about": {
        "orchestrator": {
            "UOFramework": "10.4",
            "keyfactor_platform_version": "10.3",
            "pam_support": true,
            "store_types": [
                {
                    "Name": "F5 Big IQ",
                    "ShortName": "F5-BigIQ",
                    "Capability": "F5-BigIQ",
                    "LocalStore": false,
                    "SupportedOperations": {
                        "Add": true,
                        "Create": false,
                        "Discovery": false,
                        "Enrollment": true,
                        "Remove": true
                    },
                    "Properties": [],
                    "EntryParameters": [],
                    "PasswordOptions": {
                        "EntrySupported": false,
                        "StoreRequired": false,
                        "Style": "Default"
                    },
                    "PrivateKeyAllowed": "Required",
                    "JobProperties": [],
                    "ServerRequired": true,
                    "PowerShell": false,
                    "BlueprintAllowed": true,
                    "CustomAliasAllowed": "Required",
                    "ClientMachineDescription": "The full URL of the F5 Big IQ device portal, such as 'https://f5bigiq.example.com'.",
                    "StorePathDescription": "The name of the partition on the F5 Big IQ device to be managed, entered in a case-sensitive manner, such as 'Common'."
                }
            ]
        }
    }
}